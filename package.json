{
  "name": "vscode-xrest-client",
  "displayName": "Xrest Client",
  "description": "Flexible REST API testing in VS Code using Javascript",
  "version": "0.0.0",
  "publisher": "RhaldKhein",
  "author": {
    "name": "RhaldKhein",
    "email": "undyingmind@gmail.com",
    "url": "https://github.com/rhaldkhein"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhaldkhein/vscode-xrest-client.git"
  },
  "bugs": {
    "url": "https://github.com/rhaldkhein/vscode-xrest-client/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/rhaldkhein/vscode-xrest-client",
  "icon": "media/images/logo-head-128.png",
  "categories": [
    "Debuggers",
    "Formatters",
    "Visualization",
    "Other"
  ],
  "keywords": [
    "REST",
    "RESTful",
    "API",
    "Client",
    "Testing",
    "Debugging"
  ],
  "main": "./out/extension.js",
  "engines": {
    "vscode": "^1.47.0"
  },
  "activationEvents": [
    "onCommand:vscode-xrest-client.sendRequest",
    "onCommand:vscode-xrest-client.showLastResponse",
    "onLanguage:javascript"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-xrest-client.sendRequest",
        "category": "Xrest Client",
        "title": "Send Request"
      },
      {
        "command": "vscode-xrest-client.showLastResponse",
        "category": "Xrest Client",
        "title": "Show Last Response"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-xrest-client.sendRequest",
        "key": "ctrl+cmd+x"
      },
      {
        "command": "vscode-xrest-client.showLastResponse",
        "key": "ctrl+cmd+z"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "vscode-xrest-client.sendRequest",
          "when": "resourceLangId == javascript",
          "group": "z_commands"
        },
        {
          "command": "vscode-xrest-client.showLastResponse",
          "when": "resourceLangId == javascript",
          "group": "z_commands"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.4",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.1",
    "@types/lodash.defaultsdeep": "^4.6.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/set-cookie-parser": "0.0.6",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "mocha": "^7.1.2",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "ejs": "^3.1.3",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "lodash.defaultsdeep": "^4.6.1",
    "minimatch": "^3.0.4",
    "set-cookie-parser": "^2.4.6"
  }
}