{
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "ordered-imports": false,
    "no-shadowed-variable": false,
    "space-before-function-paren": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "curly": false,
    "prefer-for-of": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "typedef": [
      true,
      "call-signature",
      "property-declaration"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ]
  }
}